int main() {
    init();
    mgba_open();
    mgba_printf("Debugging Initialized :)");
    while (1) {
        oldButtons = buttons;
        buttons = (*(volatile unsigned short *)0x04000130);
        switch (state) {
            case START_MENU:
                doStartMenu();
                break;

            case INFO_MENU:
                doInfoMenu();
                break;

            case ABOUT_MENU:
                doAboutMenu();
                break;

            case CONTROLS_MENU:
                doControlsMenu();
                break;

            case GAME:
                doGame();
                break;

            case PAUSE:
                doPause();
                break;

            case DEATH_ENERGY:
                doDeathEnergy();
                break;

            case UPGRADE_MENU:
                doUpgradeMenu();
                break;

            case DEATH_PLASTIC:
                doDeathPlastic();
                break;

            default:
                break;
        }
    }
}

void init() {
    (*(volatile unsigned short *)0x4000000) = 0 | (1<<8) | (1<<12);

    setupInterrupts();
    setupSounds();

    int shells_owned = 0;
    u16 oldButtons = 0;
    u16 buttons = (*(volatile unsigned short *)0x04000130);

    hideSprites();
    goStartMenu();
}
